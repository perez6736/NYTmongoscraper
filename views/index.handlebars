<div class="container">

    <!-- Jumbotron  -->
    <div class="jumbotron">
      <h1>New York Times Scraper</h1>
    </div>

    <div class="row">
      <!-- Results Panel -->
      <div class="col-lg-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Search Results</h3>
          </div>
          <div class="panel-body" id="well-section">
              {{#each articles}}
              <div data-id = {{id}}>{{title}}</div>
              {{/each}}
          </div>
        </div>
      </div>
    </div>

  </div>